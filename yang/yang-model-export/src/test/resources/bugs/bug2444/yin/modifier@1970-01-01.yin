<module xmlns="urn:ietf:params:xml:ns:yang:yin:1" xmlns:m="modifier"
    name="modifier">
    <yang-version value="1.1"></yang-version>
    <namespace uri="modifier"></namespace>
    <prefix value="m"></prefix>
    <leaf name="my-leaf">
        <type name="string">
            <pattern value=".*">
                <modifier value="invert-match"></modifier>
            </pattern>
        </type>
    </leaf>
</module>