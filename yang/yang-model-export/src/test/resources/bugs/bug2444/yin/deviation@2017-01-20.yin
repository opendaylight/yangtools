<module xmlns="urn:ietf:params:xml:ns:yang:yin:1" xmlns:dev="deviation"
    xmlns:dev-tgt="deviation-target" name="deviation">
    <yang-version value="1.1"></yang-version>
    <namespace uri="deviation"></namespace>
    <prefix value="dev"></prefix>
    <import module="deviation-target">
        <prefix value="dev-tgt"></prefix>
        <revision-date date="2017-01-20"></revision-date>
    </import>
    <revision date="2017-01-20"></revision>
    <extension name="custom-property">
        <argument name="name"></argument>
    </extension>
    <deviation target-node="/dev-tgt:my-leaf-list">
        <deviate value="add">
            <config value="false"></config>
            <min-elements value="5"></min-elements>
            <max-elements value="10"></max-elements>
            <default value="100"></default>
            <default value="200"></default>
            <units name="hours"></units>
        </deviate>
    </deviation>
    <deviation target-node="/dev-tgt:my-list">
        <deviate value="add">
            <unique tag="my-leaf-a my-leaf-b"></unique>
            <unique tag="my-leaf-c my-leaf-d"></unique>
        </deviate>
    </deviation>
    <deviation target-node="/dev-tgt:my-choice">
        <deviate value="add">
            <default value="c2"></default>
        </deviate>
    </deviation>
    <deviation target-node="/dev-tgt:my-rpc/dev-tgt:input">
        <deviate value="add">
            <must condition="day or night"></must>
            <must condition="black and white"></must>
        </deviate>
    </deviation>
    <deviation target-node="/dev-tgt:my-rpc/dev-tgt:output">
        <deviate value="add">
            <must condition="day or night"></must>
            <must condition="black and white"></must>
        </deviate>
    </deviation>
    <deviation target-node="/dev-tgt:my-notification">
        <deviate value="add">
            <must condition="day or night"></must>
            <must condition="black and white"></must>
        </deviate>
    </deviation>
    <deviation target-node="/dev-tgt:my-anyxml">
        <deviate value="add">
            <mandatory value="true"></mandatory>
            <dev:custom-property dev:name="arg"></dev:custom-property>
            <dev:custom-property dev:name="another arg"></dev:custom-property>
        </deviate>
    </deviation>
</module>