<module xmlns="urn:ietf:params:xml:ns:yang:yin:1" xmlns:ac="action"
    name="action">
    <yang-version value="1.1"></yang-version>
    <namespace uri="action"></namespace>
    <prefix value="ac"></prefix>
    <extension name="my-ext">
        <argument name="my-arg"></argument>
    </extension>
    <container name="root">
        <action name="a">
            <status value="deprecated"></status>
            <description>
                <text>decs</text>
            </description>
            <reference>
                <text>ref</text>
            </reference>
            <typedef name="my-type">
                <type name="string"></type>
            </typedef>
            <grouping name="my-grp">
                <container name="con-in-grp"></container>
            </grouping>
            <input>
                <container name="i"></container>
            </input>
            <output>
                <container name="o"></container>
            </output>
            <ac:my-ext ac:my-arg="arg-val"></ac:my-ext>
        </action>
    </container>
</module>