module foo {
    namespace "foo";
    prefix foo;
    yang-version 1;

    import semantic-version { prefix sv; sv:semantic-version "1.0.0"; }
    import bar { prefix bar; sv:semantic-version "1.2.5"; }

    revision "2016-02-01" {
        description "Initial version";
    }
    sv:semantic-version "01.50.02";

    augment "/bar:root/bar:test-container" {
        leaf accepted {
            type boolean;
            default true;
        }
    }
}
