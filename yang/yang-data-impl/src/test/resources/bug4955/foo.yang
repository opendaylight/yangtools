module foo {
    yang-version 1;
    namespace "foo";
    prefix foo;

    revision 2016-05-17 {
        description "test";
    }

    container task-container {
        list task {
            key "task-id";
            unique "my-leaf-1 my-leaf-2";
            unique "my-leaf-1 my-container/my-leaf-3";

            leaf task-id {
                type string;
            }
            leaf my-leaf-1 {
                type string;
            }
            leaf my-leaf-2 {
                type string;
            }
            container my-container {
                leaf my-leaf-3 {
                    type string;
                }
            }
        }
    }
}
