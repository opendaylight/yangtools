        leaf curAccountPort {
          type uint32;
          config false;
          description
            "Accounting server port being used.";
        }
        leaf priAccountPort {
          type uint32;
          config false;
          description
            "Port of the primary accounting server.";
        }
        leaf curAccountSrv {
          type inet:ipv4-address-no-zone;
          config false;
          description
            "Accounting server port being used.";
        }
        leaf priAccountSrv {
          type inet:ipv4-address-no-zone;
          config false;
          description
            "Primary accounting server.";
        }
        leaf acctStopPktNo {
          type uint32;
          config false;
          description
            "Number of responses to accounting-stop packets.";
        }
        leaf acctRspPassNo {
          type uint32;
          config false;
          description
            "Number of responses to accounting-pass packets.";
        }
        leaf acctRspNo {
          type uint32;
          config false;
          description
            "Number of responses to accounting requests.";
        }
        leaf acctSrvOpenNo {
          type uint32;
          config false;
          description
            "Number of times that the HWTACACS client connected to the accounting server.";
        }
        leaf acctStartPktNo {
          type uint32;
          config false;
          description
            "Number of responses to accounting-start packets.";
        }
        leaf acctReqNo {
          type uint32;
          config false;
          description
            "Number of accounting requests sent to the server.";
        }
        leaf acctSrvClseNo {
          type uint32;
          config false;
          description
            "Number of times that the HWTACACS client disconnected from the accounting server.";
        }
        leaf acctRspErrNo {
          type uint32;
          config false;
          description
            "Number of abnormal accounting responses received from the server.";
        }
        leaf acctFollowNo {
          type uint32;
          config false;
          description
            "Number of accounting Follow responses received from server.";
        }
        leaf acctPndreqNo {
          type uint32;
          config false;
          description
            "Number of accounting requests that are not processed.";
        }
        container hwTacSrvCfgs {
          description
            "A set of HWTACACS servers.";
          list hwTacSrvCfg {
            key "serverIpAddress serverType isSecondaryServer vpnName isPublicNet";
            max-elements  "32";
            description
              "HWTACACS IPV4 server. A maximum 32 servers can be configured in one template ";
            leaf serverIpAddress {
              type inet:ipv4-address-no-zone;
              description
                "Server IPv4 address. Must be a valid unicast IP address.";
            }
            leaf serverType {
              type hwtacacsSrvType;
              description
                "Server type: authentication/authorization/accounting/common.";
            }
            leaf isSecondaryServer {
              type boolean;
              description
                "Whether the server is secondary. By default, a server is a secondary server.";
            }
            leaf vpnName {
              type leafref {
                path "/l3vpn:l3vpn/l3vpn:l3vpncomm/l3vpn:l3vpnInstances/l3vpn:l3vpnInstance/l3vpn:vrfName";
              }
              description
                "VPN instance name.";
            }
            leaf isPublicNet {
              type boolean;
              description
                "Set the public-net.";
            }
            leaf serverPort {
              type uint32 {
                range "1..65535";
              }
              default "49";
              description
                "Server port. Value range: 1-65535. The default port number is 49.";
              ext:allowDelete "true";
            }
            leaf isMuxModeEnable {
              type boolean;
              default "false";
              description
                "Whether the MUX mode is enabled for the server. By default, the MUX mode is disabled.";
              ext:allowDelete "true";
            }
            leaf serverCurrentState {
              type hwtacacsSrvState;
              config false;
              description
                "Server running status.";
            }
            leaf isCurrentSrv {
              type boolean;
              default "false";
              config false;
              description
                "Whether the server is being used.";
            }
            leaf sharedKey {
              type pub-type:passwordExtend {
                length "1..255";
              }
              description
                "Shared key for a HWTACACS server. Configuring a shared key improves the communication security between a router and HWTACACS server. By default, no shared key is configured.";
              ext:allowDelete "true";
            }
            leaf authenSrvOpenNo {
              type uint32;
              config false;
              description
                "Number of times that the HWTACACS client connected to the authentication server.";
            }
            leaf authenSrvClseNo {
              type uint32;
              config false;
              description
                "Number of times that the HWTACACS client disconnected from the authentication server.";
            }
            leaf authenReqNo {
              type uint32;
              config false;
              description
                "Number of authentication requests. ";
            }
            leaf authenRspNo {
              type uint32;
              config false;
              description
                "Number of authentication responses.";
            }
            leaf authorSrvOpenNo {
              type uint32;
              config false;
              description
                "Number of times that the HWTACACS client connected to the authorization server.";
            }
            leaf authorSrvClseNo {
              type uint32;
              config false;
              description
                "Number of times that the HWTACACS client disconnected from the authorization server.";
            }
            leaf authorReqNo {
              type uint32;
              config false;
              description
                "Number of authorization requests. ";
            }
            leaf authorRspNo {
              type uint32;
              config false;
              description
                "Number of authorization responses.";
            }
            leaf acctReqNo {
              type uint32;
              config false;
              description
                "Number of accounting requests. ";
            }
            leaf acctRspNo {
              type uint32;
              config false;
              description
                "Number of accounting responses.";
            }
            leaf acctSrvOpenNo {
              type uint32;
              config false;
              description
                "Number of times that the HWTACACS client connected to the accounting server.";
            }
            leaf acctSrvClseNo {
              type uint32;
              config false;
              description
                "Number of times that the HWTACACS client disconnected from the accounting server.";
            }
          }
        }
        container hwTacIpv6SrvCfgs {
          description
            "A set of HWTACACS servers.";
          list hwTacIpv6SrvCfg {
            key "serverIpAddress serverType isSecondaryServer vpnName";
            max-elements  "32";
            description
              "HWTACACS IPV6 server. A maximum 32 servers can be configured in one template ";
            leaf serverIpAddress {
              type inet:ipv6-address-no-zone;
              description
                "Server IPv6 address. Must be a valid unicast IP address.";
            }
            leaf serverType {
