module huawei-driver-deviations-CH-NE-X3 {
  yang-version 1;
  namespace "urn:huawei:yang:huawei-driver-deviations-CH-NE-X3";
  prefix driver-devs-CH-NE-X3;
  
  import huawei-driver {
    prefix driver;
  }
  
  import huawei-devm {
    prefix devm;
  }
  
  organization
    "Huawei Technologies Co., Ltd.";
  contact
    "Huawei Industrial Base
    Bantian, Longgang
    Shenzhen 518129
    People's Republic of China
    Website: http://www.huawei.com
    Email: support@huawei.com";
  description
    "Configuration deviations of the driver file.";
  revision 2019-04-01 {
    description
      "The modifications in the schema file are synchronized to the YANG file.";
    reference
      "Huawei private.";
  }
  
/*
 typedef deviations begin
*/
 typedef trap-type-deviations {
    type enumeration {
      enum "base-trap" {
        value 1;
        description
          "Base-trap.";
      }
      enum "entity-trap" {
        value 4;
        description
          "Entity-trap.";
      }
    }
    description
      "Information of enum type trap-type.";
  }
  
  typedef pwr-supply-temp-type-deviations {
    type enumeration {
      enum "temphigh" {
        value 55;
        description
          "55.";
      }
    }
    description
      "Information of enum type pwr-supply-temp-type.";
  }
  
  typedef fan-workmode-deviations {
    type enumeration {
      enum "auto" {
        value 1;
        description
          "Automatic.";
      }
      enum "manual" {
        value 2;
        description
          "Percent.";
      }
      enum "silent" {
        value 4;
        description
          "Silent.";
      }
      enum "full" {
        value 5;
        description
          "Full.";
      }
      enum "minimum-percent" {
        value 9;
        description
          "Minimum-percent.";
      }
    }
    description
      "Information of enum type fan-workmode.";
  }
  
  deviation /devm:devm/devm:chassiss/devm:chassis/driver:fan-attribute/driver:workmode {
    deviate replace {
      type driver-devs-CH-NE-X3:fan-workmode-deviations;
    }
  }
  deviation /driver:driver/driver:global-attribute/driver:system-trap {
    deviate replace {
      type driver-devs-CH-NE-X3:trap-type-deviations;
    }
  }
  deviation /devm:devm/devm:chassiss/devm:chassis/driver:power-supply-attribute/driver:power-supply-temp {
    deviate replace {
      type driver-devs-CH-NE-X3:pwr-supply-temp-type-deviations;
    }
  }
/*
 typedef deviations end
*/
/*
 deviation add begin
*/
  deviation "/driver:driver/driver:temperatures/driver:temperature" {
    deviate add {
        must "(minor and major and fatal) or (not(minor) and not(major) and not(fatal))";
    }
  }
  deviation "/driver:driver/driver:voltageinfos/driver:voltageinfo" {
    deviate add {
        must "(major-voltage-threshold-low and major-voltage-threshold-high and fatal-voltage-threshold-low and fatal-voltage-threshold-high) or (not(major-voltage-threshold-low) and not(major-voltage-threshold-high) and not(fatal-voltage-threshold-low) and not(fatal-voltage-threshold-high))";
    }
  }
/*
 deviation add end
*/

/*
