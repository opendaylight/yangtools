[project]
	description = OpenDaylight YANG Tools
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[access "refs/*"]
	owner = group ldap/gerrit-yangtools-commiters
	read = group Anonymous Users
[access "refs/for/refs/*"]
	rebase = group Project Owners
[access "refs/heads/*"]
	rebase = group Project Owners
[submit]
	mergeContent = false
	action = rebase if necessary
