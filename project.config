[project]
	description = OpenDaylight YANG Tools
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[access "refs/*"]
	owner = group ldap/gerrit-yangtools-commiters
	read = group GitHub Replication
[access "refs/for/refs/*"]
	rebase = group Project Owners
[access "refs/heads/*"]
	rebase = group Project Owners
[submit]
	mergeContent = false
	action = rebase if necessary
[allowSuperproject "releng/autorelease"]
	matching = refs/heads/stable/beryllium:refs/heads/stable/beryllium
