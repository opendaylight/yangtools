            </vs-tlv>
         </tlvs>
         <delegate>true</delegate>
         <processing-rule>false</processing-rule>
         <ignore>false</ignore>
         <operational>true</operational>
         <sync>false</sync>
         <plsp-id>40</plsp-id>
         <remove>false</remove>
        </lsp>
      </reported-lsp>
      <ip-address>39.39.39.39</ip-address>
     </path-computation-client>
     <node-id>pcc://39.39.39.39</node-id>
